<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="book" namespace="/book" extends="edu-default">
        <action name="buyInfo" class="com.shangde.edu.book.action.BookAction">
        	<result name="success" type="dispatcher">/back/jsp/book/sf2011.jsp</result>            
           <result name="buyinfolist" type="dispatcher">/back/jsp/book/buyInfoList.jsp</result>
		</action>
		<action name="book" class="com.shangde.edu.book.action.BookAction">
            <result name="addbook" type="dispatcher">/back/jsp/book/book_add.jsp</result>
            <result name="boolist" type="dispatcher">/back/jsp/book/book_list.jsp</result>
            <result name="updatebook" type="dispatcher">/back/jsp/book/book_update.jsp</result>
             <result name="bookinfo" type="dispatcher">/back/jsp/book/book_info.jsp</result>
		</action>

    </package>
</struts>